// Input: CLK
NET "CLK" CLOCK_DEDICATED_ROUTE = FALSE;
NET "CLK" LOC = "B8"; // central button

// Input: CLR
NET "CLR" LOC = "C9"; // down (BTND)

// Input: D
NET "D" LOC = "T5"; // left most button

// Output: Q[3:0]
NET "Q[3]" LOC = "T11"; // left most LED
NET "Q[2]" LOC = "R11";
NET "Q[1]" LOC = "N11";
NET "Q[0]" LOC = "M11";

// Output: DR
NET "DR" LOC = "U16"; // right most LED
